    "concurrently": "^7.3.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
